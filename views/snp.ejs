<h2>Check out some local spots!</h2>

<% for (place of places){%>


    <div class="ui-card">
        <h2><%= place.name %></h2>
        <img src="<%= place.store_pic%>" alt="">

        <h3><%= place.services%></h3>
        <p>
            <%= place.location %>
        </p>
        <p>
            This location is rated at : <%= place.rating %>
        </p>
        <p>
            posted by: <%= place.user_id %>
        </p>
    </div>
<%}%>