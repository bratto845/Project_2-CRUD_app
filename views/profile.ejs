<section class= "profile_page">
    <div class="profile_card">  
        <div class="profile-pics">      
            <img src="<%= user.pet_pic %>" class="pet_pic" alt=""> 
            <img src="<%= user.profile_pic %>" class ="profile_pic" alt="">
            <h1> <%= user.user_name %></h1>
            <P>This is me and my dog!</P>
            <p>FetchMate objective: <%= user.category%></p>
            <div class="btns">
                <form action="/profile/<%=user.id%>?_method=DELETE" method="POST">
                    <button class="dlt_btn" type="submit">Delete</button>
                </form>
                <form action="/profile/<%=user.id%>/edit">
                    <button class="edt_btn" type="submit">Edit Profile</button>
                </form>
            </div>
        </div>
    

    </div>
</section>
